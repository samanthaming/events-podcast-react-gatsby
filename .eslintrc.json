{
  "extends": ["airbnb", "prettier"],
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "react/prefer-stateless-function": "off",
    "react/no-danger": "off",
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0
  },
  "settings": {
    "import/core-modules": []
  },
  "env": {
    "browser": true
  }
}
